{
  "name": "my-zeronet-site",
  "version": "1.0.0",
  "description": "ZeroNet Mithril Template",
  "private": true,
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "build:prod": "webpack -p",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint 'src/**/*.ts' --fix",
    "test": "ospec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anoadragon453/zeronet-site-template-mithril"
  },
  "keywords": [
    "zeronet",
    "zeronetwork"
  ],
  "author": "Andrew Morgan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anoadragon453/zeronet-site-template-mithril/issues"
  },
  "homepage": "https://github.com/anoadragon453/zeronet-site-template-mithril#readme",
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/mithril": "^1.1.14",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.5",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^5.5.0",
    "mithril": "^1.1.7",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "zeroframe": "^1.2.0"
  }
}
